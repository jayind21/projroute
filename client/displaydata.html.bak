<template name="data2015">
  <h2>Document List 2015</h2>
  <table style="width:100%">
  <tr>
    <td>Active</td>
	<td>Active Date</td>
	<td>Expiry Date</td>
  </tr>
  {{#each messages}}
   <tr>
   <td>{{active}}</td>
   <td>{{active_date}}</td>
   <td>{{expiry_date}}</td>
   </tr>
  {{/each}}
  </table>
</template>

